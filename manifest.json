{
  "manifest_version": 2,
  "name": "External Resource Checker",
  "version": "1.0",
  "description": "Check for libraries/external resources on a website based on known sources.",
  "icons": {
    "48": "icons/icon-48.svg",
    "96": "icons/icon-96.svg"
  },
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": {
      "48": "icons/icon-48.svg",
      "96": "icons/icon-96.svg"
    },
    "default_popup": "popup/popup.html",
    "default_title": "My Extension"
  }
}
