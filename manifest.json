{
  "manifest_version": 2,
  "name": "External Resource Checker",
  "version": "1.2",
  "description": "Check for libraries/external resources on a website based on known sources.",
  "icons": {
    "48": "icons/icon-48.png",
    "96": "icons/icon-96.png"
  },
  "permissions": ["activeTab", "<all_urls>"],
  "browser_action": {
    "default_icon": {
      "48": "icons/icon-48.png",
      "96": "icons/icon-96.png"
    },
    "default_popup": "popup/popup.html",
    "default_title": "External Resource Checker"
  }
}
